{
  "name": "website",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vitepress dev",
    "prebuild": "pnpm run sponsors",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "lint": "oxlint -W correctness -W no-unused-vars --deny-warnings",
    "lint:fix": "pnpm lint --fix && typos -w && pnpm fmt --write .",
    "typecheck": "tsc && vue-tsc",
    "fmt": "dprint fmt",
    "sponsors": "degit Boshen/sponsors temp --force && mv temp/sponsors.svg public/sponsors.svg",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@vueuse/core": "^14.0.0",
    "degit": "^2.8.4",
    "dprint": "^0.50.1",
    "esbuild": "0.25.11",
    "feed": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "16.2.6",
    "oxc-minify": "^0.95.0",
    "oxlint": "^1.14.0",
    "typescript": "~5.9.2",
    "vitepress": "2.0.0-alpha.12",
    "vitepress-plugin-group-icons": "^1.6.3",
    "vue": "^3.5.21",
    "vue-tsc": "^3.1.2"
  },
  "lint-staged": {
    "*": "dprint fmt --staged"
  },
  "packageManager": "pnpm@10.19.0",
  "volta": {
    "node": "22.14.0"
  },
  "pnpm": {
    "overrides": {
      "vitepress>vite": "npm:rolldown-vite@latest"
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
